<% verify.forEach(function(name, index){ %>
<div id="verify_<%= index %>">
    <div class="row">
        <div class="span3">
            <fieldset>
                <div class="control-group">
                    <label class="control-label">Type</label>
                    <div class="controls controls-row">
                        <input name="check[verify[type]" type="text"  class="span1" value="<%= name["type"] %>"/>
                    </div>
                </div>
            </fieldset>
        </div>
        <div class="span4">
            <fieldset>
                <div class="control-group">
                    <label class="control-label"  style="width:65px;">Path</label>
                    <div class="controls controls-row"  style="margin-left: 80px">
                        <input name="check[verify[param1]" type="text"  value="<%= name["param1"] || "" %>"/>
                    </div>
                </div>
            </fieldset>
        </div>
        <div class="span3">
            <fieldset>
                <div class="control-group">
                    <label class="control-label"  style="width:65px;">Expect</label>
                    <div class="controls controls-row"  style="margin-left: 80px">
                        <input class="span2" name="check[verify[param2]" type="text"  value="<%= name["param2"] || "" %>"/>
                    </div>
                </div>
            </fieldset>
        </div>
        <a class="btn" value="<%= index %>" name="deleteVerify"><i class="icon-remove"></i></a>
    </div>
</div>
<% }) %>


